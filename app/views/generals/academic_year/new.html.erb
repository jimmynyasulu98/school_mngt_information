<%= render 'shared/navbar' %>

<%= form_with model: @academic_year, url: academic_year_index_path do |form| %>
  <div class= "container d-flex">
    <%= form.text_field :name %>

    <%= form.date_field :start_date %>
    <%= form.date_field :end_date%>

    <%= form.button :submit  %>
  </div>
<% end %>
